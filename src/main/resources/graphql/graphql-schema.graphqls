scalar Date

# クエリ定義
type Query {
    fetchEmployeeById(employeeId: ID): EmployeeResponse
    fetchEmployeeAll: [EmployeeResponse]
}
# タイプ定義
type EmployeeResponse {
    employeeId: ID!
    employeeName: String
    department: Department
    gender: String
    personal: Personal
}

type Department {
    departmentId: ID!
    departmentName: String
}

type Personal {
    employeeId: ID!
    birthday: Date
    telephoneNumber: String
    mailAddress: String
}
